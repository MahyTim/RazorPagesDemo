@page
@{
    @functions
    {
        public IActionResult OnGetSomeData()
        {
            return new OkObjectResult(new
            {
                Here = "Is some data",
                AndHere = new [] {"Is", "Some", "More"}
            });
        }
    }
}

<pre id="target"></pre>
<script>
    fetch('@Url.Page("", "SomeData")')
    .then(x => x.text())
    .then(x => document.getElementById('target').innerHTML = x)
</script>
